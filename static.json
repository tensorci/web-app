{
  "root": "build/",
  "routes": {
    "/favicon.ico": "/favicon.ico",
    "/fonts/*": "/fonts/",
    "/static/*": "/static/",
    "/**": "/index.html"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "/fonts/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  },
  "proxies": {
    "/api/": {
      "origin": "http://localhost:5000/api"
    },
    "/socket.io/": {
      "origin": "http://localhost:5000/socket.io"
    }
  }
}
