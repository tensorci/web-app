{
  "root": "build/",
  "https_only": true,
  "routes": {
     "/favicon.ico": "/favicon.ico",
     "/fonts/*": "/fonts/",
     "/static/*": "/static/",
     "/**": "/index.html"
  },
  "headers": {
    "/**": {
      "Cache-Control": "no-store, no-cache"
    }
  },
  "proxies": {
    // Proxy for API request
    "/api/": {
      "origin": "https://api.tensorci.com/api"
    }
  }
}